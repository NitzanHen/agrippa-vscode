{
  "name": "agrippa",
  "icon": "agrippa.png",
  "displayName": "Agrippa",
  "publisher": "nitzanhen",
  "description": "Generate frontend components in a heartbeat with Agrippa",
  "homepage": "https://www.agrippa.dev/",
  "license": "MIT",
  "version": "0.0.3",
  "preview": true,
  "engines": {
    "vscode": "^1.79.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "workspaceContains:**/agrippa.config.mjs"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "agrippa.init",
        "title": "Agrippa: Create a config file"
      },
      {
        "command": "agrippa.generate",
        "title": "Generate a component here..."
      }
    ],
    "menus": {
      "explorer/context": [
        {
          "command": "agrippa.generate",
          "group": "7_modification@3"
        }
      ]
    }
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/NitzanHen/agrippa-vscode.git"
  },
  "scripts": {
    "vscode:prepublish": "yarn run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "yarn run compile && yarn run lint",
    "lint": "eslint src --ext ts"
  },
  "devDependencies": {
    "@types/node": "20.2.5",
    "@types/vscode": "^1.79.0",
    "@typescript-eslint/eslint-plugin": "^5.59.8",
    "@typescript-eslint/parser": "^5.59.8",
    "@vscode/test-electron": "^2.3.2",
    "eslint": "^8.41.0",
    "typescript": "^5.1.3"
  },
  "dependencies": {
    "agrippa": "^2.0.3",
    "glob": "^10.2.7"
  },
  "keywords": [
    "productivity",
    "component",
    "generator",
    "react"
  ]
}
